{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/webdav-proxy/[...path].ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/webdav-proxy/(.*)",
      "destination": "/api/webdav-proxy/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}